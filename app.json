{
  "expo": {
    "name": "Repl.it",
    "slug": "replit-mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "version": "0.0.1",
    "orientation": "default",
    "icon": "./assets/images/app/icon.png",
    "splash": {
      "image": "./assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.replit.mobile"
    },
    "android": {
      "package": "com.replit.mobile",
      "adaptiveIcon": {
        "backgroundColor": "#333333",
        "foregroundImage": "./assets/images/app/foreground.png"
      }
    },
    "description": "The mobile app for Repl.it. It's a work in progress and this is meant for testers.",
    "githubUrl": "https://github.com/kognise/repl.it-mobile"
  }
}
